    <decisions>
        <name>Did_Dental_Grade_Change</name>
        <label>Did Dental Grade Change?</label>
        <locationX>526</locationX>
        <locationY>372</locationY>
        <defaultConnector>
            <targetReference>Did_Exam_Task_List_Change</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Dental Grade Not Changed</defaultConnectorLabel>
        <rules>
            <name>Dental_Grade_Changed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Dental_Grade__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>2-3</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Dental_Grade__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>3</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Dental_Grade__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>3-4</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Dental_Grade__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>4</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Dental_Procedure</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>Dental Grade Changed</label>
        </rules>
    </decisions>
