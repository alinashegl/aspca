    <formulas>
        <name>animalBCS</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISPICKVAL({!$Record.Avian_BCS__c}, &quot;&quot;)), TEXT({!$Record.Avian_BCS__c}), TEXT({!$Record.BCS__c}))</expression>
    </formulas>
    <formulas>
        <name>dentalStatus</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!$Record.Exam_Type__c}, &quot;Forensic&quot;), &quot;Requested by Forensics&quot;, &quot;Requested by Medical&quot;)</expression>
    </formulas>
    <formulas>
        <name>oneWeek</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 7</expression>
    </formulas>
    <formulas>
        <name>twentyEightDays</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 28</expression>
    </formulas>
    <formulas>
        <name>twoWeeks</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 14</expression>
    </formulas>
    <formulas>
        <name>weightKGS</name>
        <dataType>Number</dataType>
        <expression>IF({!$Record__Prior.Weight_in_kgs_Num__c} != {!$Record.Weight_in_kgs_Num__c}, {!$Record.Weight_in_kgs_Num__c}, {!$Record.Weight_in_kgs_Calc__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
