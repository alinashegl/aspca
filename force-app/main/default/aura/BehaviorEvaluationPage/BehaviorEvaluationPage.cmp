                                                <td width="6%"></td>
                                                <td width="7%"></td>
                                                <td width="7%"></td>
                                                <td width="7%"></td>
                                                <td width="7%"></td>
                                                <td width="25%"></td>
                                                <td width="22%">TB = Touch Body</td>
                                                <td width="22%">TN = Touch Neck</td>
                                            </tr>
                                            <tr width="100%">
                                                <td width="6%"></td>
                                                <td width="7%"><strong>TB</strong></td>
                                                <td width="7%"><strong>TN</strong></td>
                                                <td width="7%"><strong>TF</strong></td>
                                                <td width="7%"><strong>PF</strong></td>
                                                <td width="25%"></td>
                                                <td width="22%">TF = Touch Face</td>
                                                <td width="22%">PF = Push Face</td>
                                            </tr>
                                        </div>
                                    </div>
                                </table>

