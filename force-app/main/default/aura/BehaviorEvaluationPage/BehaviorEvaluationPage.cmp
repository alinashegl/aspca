<aura:component
        description="BehaviorEvaluationPage"
        implements="flexipage:availableForAllPageTypes,force:hasRecordId"
        access="public"
        controller="BehaviorEvaluationPageController">
    <!-- Build Attributes to Call from Javascript Controller -->
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="animalId" type="String" />
    <aura:attribute name="tabs" type="Object[]" /> <!-- TODO: Remove this - try and Refactor -->
    <aura:attribute name="behaviorRecord" type="Object"/>
    <aura:attribute name="behaviorReport" type="Object[]"/>
    <!-- ADULT ANIMAL PROPERTIES -->
    <aura:attribute name="behaviorInKennelTest" type="Object[]" />
    <aura:attribute name="behaviorOnLeashTest" type="Object[]" />
    <aura:attribute name="socialBehaviorTestPart1" type="Object[]" />
    <aura:attribute name="socialBehaviorTestPart2" type="Object[]" />
    <aura:attribute name="pleasantTouchTest" type="Object[]" />
    <aura:attribute name="unpleasantTouchTest" type="Object[]" />
    <aura:attribute name="playTestPart1" type="Object[]"/>
    <aura:attribute name="playTestPart2" type="Object[]" />
    <aura:attribute name="tagTest" type="Object[]" />
    <aura:attribute name="resourceGuardingTestPart1" type="Object[]" />
    <aura:attribute name="resourceGuardingTestPart1TF" type="Object[]" />
    <aura:attribute name="resourceGuardingTestPart1TN" type="Object[]" />
    <aura:attribute name="resourceGuardingTestPart1TB" type="Object[]" />
    <aura:attribute name="resourceGuardingTestPart2" type="Object[]" />
    <aura:attribute name="resourceGuardingTestPart2TF" type="Object[]" />
    <aura:attribute name="resourceGuardingTestPart2TN" type="Object[]" />
    <aura:attribute name="resourceGuardingTestPart2TB" type="Object[]" />
    <aura:attribute name ="toddlerDollTestPart1" type="Object[]" />
    <aura:attribute name="toddlerDollTestP2" type="Object[]" />
    <aura:attribute name="scoldingPersonTestP1" type="Object[]" />
    <aura:attribute name="scoldingPersonTestP2" type="Object[]" />
    <aura:attribute name="fakeDogInteractionTest" type="Object[]" />
    <aura:attribute name="sameSexDogTestPart1" type="Object[]" />
    <aura:attribute name="sameSexDogTestPart2" type="Object[]" />
    <aura:attribute name="sameSexDogTestPart3" type="Object[]" />
    <aura:attribute name="oppositeSexDogTestPart1" type="Object[]" />
    <aura:attribute name="oppositeSexDogTestPart2" type="Object[]" />
    <aura:attribute name="oppositeSexDogTestPart3" type="Object[]" />
    <aura:attribute name="tabsAttributeMap" type="Map"/>

    <!-- PUPPY TEST PROPERTIES -->
    <aura:attribute name="puppyBehaviorInKennel" type="Object[]" />
    <aura:attribute name="puppyBehaviorOnLeash" type="Object[]" />
    <aura:attribute name="puppySocialBehaviorPart1" type="Object[]" />
    <aura:attribute name="puppySocialBehaviorPart2" type="Object[]" />
    <aura:attribute name="puppyPlayTestTugPart1" type="Object[]" />
    <aura:attribute name="puppyPlayTestTugPart2" type="Object[]" />
    <aura:attribute name="puppyPlayTestTag" type="Object[]" />
    <aura:attribute name="puppyRestraint" type="Object[]" />
    <aura:attribute name="puppyResourceGuardingPart1" type="Object[]" />
    <aura:attribute name="puppyResourceGuardingPart2" type="Object[]" />
    <aura:attribute name="puppyChildDollPart1" type="Object[]" />
    <aura:attribute name="puppyChildDollPart2" type="Object[]" />
    <aura:attribute name="puppyDogInteractionPart1" type="Object[]" />
    <aura:attribute name="puppyDogInteractionPart2" type="Object[]" />
    <aura:attribute name="puppyDogInteractionPart3" type="Object[]" />

    <!--   Page Action Properties -->
    <aura:attribute name="tabId" type="String" default="behaveInKennel" />
    <aura:attribute name="subTabId" type="String" />
    <aura:attribute name="selection" type="String" />
    <aura:attribute name="showModal" type="Boolean" default="false" />
    <aura:attribute name="iShowModal" type="Boolean" default="false" />
    <aura:attribute name="fShowModal" type="Boolean" default="false" />
    <aura:attribute name="IsAdult" type="Boolean" default="false" />
    <aura:attribute name="IsPuppy" type="Boolean" default="false" />
    <aura:attribute name="IsDogOnly" type="Boolean" default="false" />
    <aura:attribute name="IsDogFighting" type="Boolean" />
    <aura:attribute name="UseCaution" type="Boolean" />
    <aura:attribute name="UnpleasantTouch1stFlank" type="Boolean" />
    <aura:attribute name="UnpleasantTouch2ndFlank" type="Boolean" />
    <aura:attribute name="PuppySingly" type="Boolean" />
    <aura:attribute name="PuppyCoHoused" type="Boolean" />
    <aura:attribute name="PuppyMuzzle1" type="Boolean" />
    <aura:attribute name="PuppyMuzzle2" type="Boolean" />
    <aura:attribute name="PuppyMuzzle3" type="Boolean" />
    <aura:attribute name="MuzzleSSD3" type="Boolean" />
    <aura:attribute name="MuzzleOSD3" type="Boolean" />
    <aura:attribute name="spinner" type="boolean" default="false"/>


    <!-- attribute for component attributes -->
    <aura:attribute name="notConcerningOptions" type="List"/>

    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:handler name="change" value="{!v.tabId}" action="{!c.handleChange}" />
    <aura:handler name="select" value="{!v.selection}" action="{!c.handleSelection}" />
    <aura:method name="handleClick"  action="{!c.handleClick}" access="public" />
    <aura:handler name="skipSaveEvt" event="c:skipSaveEvent" action="{!c.handleSkipSaveEvent}"/>
    <aura:attribute name="behaviorEvaluation" type="Object"/>
    <!--loading spinner start-->
    <aura:if isTrue="{!v.spinner}">
        <div aura:id="spinnerId" class="slds-spinner_container">
            <div class="slds-spinner--brand  slds-spinner slds-spinner--large slds-is-relative" role="alert">
                <span class="slds-assistive-text">Loading...</span>
                <div class="slds-spinner__dot-a"></div>
                <div class="slds-spinner__dot-b"></div>
            </div>
        </div>
    </aura:if>
    <!-- Loading spinner end-->

    <lightning:notificationsLibrary aura:id="lib" />
    <lightning:overlayLibrary aura:id="overlayLib" />



        <lightning:tabset variant="vertical" selectedTabId="{!v.tabId}" aura:id="tabs">
            <lightning:recordEditForm class="slds-grid slds-theme_shade" objectApiName="Behavior_Evaluation__c"
                                      recordId="{!v.recordId}">
                <!--<lightning:buttonGroup class="position: relative;" >
                    <lightning:button name="prevTab" value="Previous Tab" label="Previous Tab" onclick="{!c.handleClick}"
                                      class="float"/>
                    <lightning:button class="float" name="nextTab" value="Next Tab" label="Next Tab" onclick="{!c.handleClick}"/>
                    <lightning:button name="showHelperDog" value="Helper Dog List" label="Helper Dog List" onclick="{!c.handleClick}"/>
                    <c:BehaviorEvaluationModal recordId="{!v.recordId}"
                                               isActive="{!v.showModal}">
                        <aura:set attribute="body">
                            <lightning:listView objectApiName="Animal__c" listName="Helper_Dog_List" rows="5"
                                                showActionBar="false" showRowLevelActions="false" showSearchBar="false" />
                        </aura:set>
                        <aura:set attribute="footer">
                            <lightning:button variant="neutral" label="Close" name="closeModal" onclick="{!c.handleClick}"/>
                        </aura:set>
                    </c:BehaviorEvaluationModal>
                </lightning:buttonGroup>-->
                <aura:if isTrue="{!!v.spinner}">
                    <lightning:layout >
                        <lightning:layoutItem size="6">
                            <div class="slds-float_left">
                                <lightning:button name="prevTab" value="Previous Tab" label="Previous Tab" onclick="{!c.handleClick}"
                                                  class="float"/>
                            </div>
                        </lightning:layoutItem>
                        <lightning:layoutItem size="6">
                            <div class="slds-float_right">
                                <lightning:button class="float" name="nextTab" value="Next Tab" label="Next Tab" onclick="{!c.handleClick}"/>
                            </div>
                        </lightning:layoutItem>
                    </lightning:layout>
                </aura:if>
                <aura:if isTrue="{!v.IsAdult}">
                    <aura:if isTrue="{!!v.IsDogOnly}">
                        <lightning:tab label="Behavior In Kennel" id = "behaveInKennel">
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12">
                                    <div class="slds-align_absolute-center" style="height:3rem">
                                        <h1 class="tabHeader">Behavior In Kennel</h1>
                                    </div>
                                    <br></br>
                                    <aura:iteration items="{!v.behaviorInKennelTest}" var="variable">
                                        <table>
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mNotConcerningOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mRedFlagOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{#variable.mAlertOptions}"
                                                                            index="{!index}"  recordId="{!v.recordId}">
                                            </c:BehaviorEvaluationOptionGroup>
                                            <aura:if isTrue="{!not(empty(variable.mInconclusiveOptions))}">
                                            &nbsp;
                                                <c:BehaviorEvaluationOptionGroup options="{!variable.mInconclusiveOptions}"
                                                                                index="{!index}"
                                                                                recordId="{!v.recordId}">
                                                </c:BehaviorEvaluationOptionGroup>
                                            </aura:if>
                                        </table>
                                    </aura:iteration>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" padding="around-small">
                                    <lightning:layout >
                                        <aura:iteration items="{!v.behaviorInKennelTest}" var="variable">
                                            <c:BehaviorEvaluationTabFooter testName="{!variable.testComments}"
                                                                        IsSkipped="{!variable.isSkipped}" recordId="{!v.recordId}"
                                                                        skipVal="{!variable.skipField}" aura:id="behEvalTabFooter"/>
                                        </aura:iteration>
                                    </lightning:layout>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </lightning:tab>
                        <lightning:tab label="Behavior On Leash" id = "bol">
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12">
                                    <div class="slds-align_absolute-center" style="height:3rem">
                                        <h1 class="tabHeader">Behavior On Leash</h1>
                                    </div>
                                    <br></br>
                                    <aura:iteration items="{!v.behaviorOnLeashTest}" var="variable">
                                        <table>
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mNotConcerningOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mRedFlagOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mAlertOptions}"
                                                                            index="{!index}"  recordId="{!v.recordId}">
                                            </c:BehaviorEvaluationOptionGroup>
                                            <aura:if isTrue="{!not(empty(variable.mInconclusiveOptions))}">
                                            &nbsp;
                                                <c:BehaviorEvaluationOptionGroup options="{!variable.mInconclusiveOptions}"
                                                                                index="{!index}"
                                                                                recordId="{!v.recordId}">
                                                </c:BehaviorEvaluationOptionGroup>
                                            </aura:if>
                                        </table>
                                    </aura:iteration>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" padding="around-small">
                                    <lightning:layout >
                                        <aura:iteration items="{!v.behaviorOnLeashTest}" var="variable">
                                            <c:BehaviorEvaluationTabFooter testName="{!variable.testComments}"
                                                                        IsSkipped="{!variable.isSkipped}" recordId="{!v.recordId}"
                                                                        skipVal="{!variable.skipField}" aura:id="behEvalTabFooter"/>
                                        </aura:iteration>
                                        &nbsp; &nbsp;
                                    </lightning:layout>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </lightning:tab>
                        <lightning:tab label="Social Behavior - Part 1" id="sbt1">
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12">
                                    <div class="slds-align_absolute-center" style="height:3rem">
                                        <h1 class="tabHeader">Social Behavior - Part 1</h1>
                                    </div>
                                    <div class="slds-align_absolute-center">
                                        <p class="tabDesc">Loose in Room, No Interaction invited</p>
                                    </div>
                                    <br></br>
                                    <aura:iteration items="{!v.socialBehaviorTestPart1}" var="variable">
                                        <table>
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mNotConcerningOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mRedFlagOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mAlertOptions}"
                                                                            index="{!index}"  recordId="{!v.recordId}">
                                            </c:BehaviorEvaluationOptionGroup>
                                            <aura:if isTrue="{!not(empty(variable.mInconclusiveOptions))}">
                                            &nbsp;
                                                <c:BehaviorEvaluationOptionGroup options="{!variable.mInconclusiveOptions}"
                                                                                index="{!index}"
                                                                                recordId="{!v.recordId}">
                                                </c:BehaviorEvaluationOptionGroup>
                                            </aura:if>
                                        </table>
                                    </aura:iteration>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" padding="around-small">
                                    <lightning:layout >
                                        <aura:iteration items="{!v.socialBehaviorTestPart1}" var="variable">
                                            <c:BehaviorEvaluationTabFooter testName="{!variable.testComments}"
                                                                        IsSkipped="{!variable.isSkipped}" recordId="{!v.recordId}"
                                                                        skipVal="{!variable.skipField}" aura:id="behEvalTabFooter"/>
                                        </aura:iteration>>
                                        &nbsp; &nbsp;
                                    </lightning:layout>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </lightning:tab>
                        <lightning:tab label="Social Behavior - Part 2" id="sbt2">
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12">
                                    <div class="slds-align_absolute-center" style="height:3rem">
                                        <h1 class="tabHeader">Social Behavior - Part 2</h1>
                                    </div>
                                    <div class="slds-align_absolute-center">
                                        <p class="tabDesc">Loose in room, tester solicits interaction</p>
                                    </div>
                                    <br></br>
                                    <aura:iteration items="{!v.socialBehaviorTestPart2}" var="variable">
                                        <table>
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mNotConcerningOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mRedFlagOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mAlertOptions}"
                                                                            index="{!index}"  recordId="{!v.recordId}">
                                            </c:BehaviorEvaluationOptionGroup>
                                            <aura:if isTrue="{!not(empty(variable.mInconclusiveOptions))}">
                                            &nbsp;
                                                <c:BehaviorEvaluationOptionGroup options="{!variable.mInconclusiveOptions}"
                                                                                index="{!index}"
                                                                                recordId="{!v.recordId}">
                                                </c:BehaviorEvaluationOptionGroup>
                                            </aura:if>
                                        </table>
                                    </aura:iteration>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" padding="around-small">
                                    <lightning:layout >
                                        <aura:iteration items="{!v.socialBehaviorTestPart2}" var="variable">
                                            <c:BehaviorEvaluationTabFooter testName="{!variable.testComments}"
                                                                        IsSkipped="{!variable.isSkipped}" recordId="{!v.recordId}"
                                                                        skipVal="{!variable.skipField}" aura:id="behEvalTabFooter"/>
                                        </aura:iteration>
                                        &nbsp; &nbsp;
                                    </lightning:layout>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </lightning:tab>
                        <lightning:tab label="Pleasant Touch" id="ptt">
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12">
                                    <div class="slds-align_absolute-center" style="height:3rem">
                                        <h1 class="tabHeader">Pleasant Touch</h1>
                                    </div>
                                    <div class="slds-align_absolute-center">
                                        <p class="tabDesc">Massage</p>
                                    </div>
                                    <br></br>
                                    <aura:iteration items="{!v.pleasantTouchTest}" var="variable">
                                        <table>
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mNotConcerningOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mRedFlagOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mAlertOptions}"
                                                                            index="{!index}"  recordId="{!v.recordId}">
                                            </c:BehaviorEvaluationOptionGroup>
                                            <aura:if isTrue="{!not(empty(variable.mInconclusiveOptions))}">
                                            &nbsp;
                                                <c:BehaviorEvaluationOptionGroup options="{!variable.mInconclusiveOptions}"
                                                                                index="{!index}"
                                                                                recordId="{!v.recordId}">
                                                </c:BehaviorEvaluationOptionGroup>
                                            </aura:if>
                                        </table>
                                    </aura:iteration>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" padding="around-small">
                                    <lightning:layout >
                                        <aura:iteration items="{!v.pleasantTouchTest}" var="variable">
                                            <c:BehaviorEvaluationTabFooter testName="{!variable.testComments}"
                                                                        IsSkipped="{!variable.isSkipped}" recordId="{!v.recordId}"
                                                                        skipVal="{!variable.skipField}" aura:id="behEvalTabFooter"/>
                                        </aura:iteration>
                                        &nbsp; &nbsp;
                                    </lightning:layout>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </lightning:tab>
                        <lightning:tab label="Unpleasant Touch" id="utt">
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12">
                                    <div class="slds-align_absolute-center" style="height:3rem">
                                        <h1 class="tabHeader">Unpleasant Touch</h1>
                                    </div>
                                    <div class="slds-align_absolute-center">
                                        <p class="tabDesc">Pinch webbing between toes twice</p>
                                    </div>
                                    <br></br>
                                    <table class="slds-box slds-p-top_medium" width="100%">
                                        <div class="grid-container">
                                            <div class="grid-item"></div>
                                            <div class="grid-item" align="left" >
                                                <tr class="spaceUnder" width="100%"></tr>
                                                <br/>
                                                <tr class="spaceUnder" width="100%" style="font-weight:900;">
                                                    <td width="2%"></td>
                                                    <td width="5%">1st</td>
                                                    <td width="5%">2nd</td>
                                                    <td width="88%"></td>
                                                </tr>
                                            </div>
                                        </div>
                                    </table>
                                            &nbsp;
                                    <table class="slds-box slds-p-top_medium" width="100%">
                                        <div class="grid-container">
                                            <div class="grid-item"></div>
                                            <div class="grid-item" align="left" >
                                                <tr class="spaceUnder" width="100%"></tr>
                                                <br/>
                                                <tr class="spaceUnder" width="100%">
                                                    <td width="0%"></td>
                                                    <td width="5%">
                                                        <lightning:input
                                                            type="checkbox-button"
                                                            onchange="{!c.handleUnpleasantTouch}"
                                                            aura:id="UnpleasantTouch1stFlankInput"
                                                            checked="{!v.UnpleasantTouch1stFlank}"
                                                        />
                                                    </td>
                                                    <td width="5%">
                                                        <lightning:input
                                                            type="checkbox-button"
                                                            onchange="{!c.handleUnpleasantTouch}"
                                                            aura:id="UnpleasantTouch2ndFlankInput"
                                                            checked="{!v.UnpleasantTouch2ndFlank}"
                                                        />
                                                    </td>
                                                    <td width="89%" style="font-weight:900;">Rear flank pinch instead</td>
                                                </tr>
                                            </div>
                                        </div>
                                    </table>
                                            &nbsp;
                                    <aura:iteration items="{!v.unpleasantTouchTest}" var="variable">
                                        <table>
                                        <aura:if isTrue="{!not(empty(variable.VC0))}">
                                            <c:BehaviorEvaluationMultipleOptionGroup colNumber="2" type="unpleasantTouch"
                                                                                    options="{!variable.VC0}"
                                                                                    index="{!index}"
                                                                                    recordId="{!v.recordId}">
                                            </c:BehaviorEvaluationMultipleOptionGroup>
                                        </aura:if>
                                            &nbsp;
                                            <c:BehaviorEvaluationMultipleOptionGroup type="unpleasantTouch" colNumber="2"
                                                                                    options="{!variable.VC1}"
                                                                                    index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationMultipleOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationMultipleOptionGroup type="unpleasantTouch" colNumber="2"
                                                                                    options="{!variable.VC2}"
                                                                                    index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationMultipleOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationMultipleOptionGroup type="unpleasantTouch" colNumber="2"
                                                                                    options="{!variable.VC3}"
                                                                                    index="{!index}"  recordId="{!v.recordId}">
                                            </c:BehaviorEvaluationMultipleOptionGroup>
                                            <aura:if isTrue="{!not(empty(variable.VC4))}">
                                            &nbsp;
                                                <c:BehaviorEvaluationMultipleOptionGroup type="unpleasantTouch" colNumber="2"
                                                                                        options="{!variable.VC4}"
                                                                                        index="{!index}"
                                                                                        recordId="{!v.recordId}">
                                                </c:BehaviorEvaluationMultipleOptionGroup>
                                            </aura:if>
                                        </table>
                                    </aura:iteration>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" padding="around-small">
                                    <lightning:layout >
                                        <aura:iteration items="{!v.unpleasantTouchTest}" var="variable">
                                            <c:BehaviorEvaluationTabFooter testName="{!variable.testComments}"
                                                                        IsSkipped="{!variable.isSkipped}" recordId="{!v.recordId}"
                                                                        skipVal="{!variable.skipField}" aura:id="behEvalTabFooter"/>
                                        </aura:iteration>
                                        &nbsp; &nbsp;
                                    </lightning:layout>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </lightning:tab>
                        <lightning:tab label="Play Tug - Part 1" id="pt1">
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12">
                                    <div class="slds-align_absolute-center" style="height:3rem">
                                        <h1 class="tabHeader">Play Tug - Part 1</h1>
                                    </div>
                                    <div class="slds-align_absolute-center">
                                        <p class="tabDesc">Tester enticed dog to tug (on loose leash)</p>
                                    </div>
                                    <br></br>
                                    <aura:iteration items="{!v.playTestPart1}" var="variable">
                                        <table>
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mNotConcerningOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mRedFlagOptions}"
                                                                            index="{!index}" recordId="{!v.recordId}" >
                                            </c:BehaviorEvaluationOptionGroup>
                                            &nbsp;
                                            <c:BehaviorEvaluationOptionGroup options="{!variable.mAlertOptions}"
                                                                            index="{!index}"  recordId="{!v.recordId}">
                                            </c:BehaviorEvaluationOptionGroup>
                                            <aura:if isTrue="{!not(empty(variable.mInconclusiveOptions))}">
                                            &nbsp;
                                                <c:BehaviorEvaluationOptionGroup options="{!variable.mInconclusiveOptions}"
                                                                                index="{!index}"
                                                                                recordId="{!v.recordId}">
                                                </c:BehaviorEvaluationOptionGroup>
                                            </aura:if>
                                        </table>
                                    </aura:iteration>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" padding="around-small">
                                    <lightning:layout >
                                        <aura:iteration items="{!v.playTestPart1}" var="variable">
                                            <c:BehaviorEvaluationTabFooter testName="{!variable.testComments}"
                                                                        IsSkipped="{!variable.isSkipped}" recordId="{!v.recordId}"
                                                                        skipVal="{!variable.skipField}" aura:id="behEvalTabFooter"/>
                                        </aura:iteration>
                                        &nbsp; &nbsp;
                                    </lightning:layout>
                                </lightning:layoutItem>
                            </lightning:layout>
                        </lightning:tab>
                        <lightning:tab label="Play Tug - Part 2" id="pt2">
                            <lightning:layout multipleRows="true">
