<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Column_Number__c</fullName>
    <externalId>false</externalId>
    <formula>if(ISPICKVAL(Category__c, &apos;Aggressive&apos;), 1,
			if(ISPICKVAL(Category__c, &apos;Arousal&apos;), 2,
						if(ISPICKVAL(Category__c, &apos;Fear&apos;), 3,
									if(ISPICKVAL(Category__c, &apos;Sociability&apos;), 4,
												if(ISPICKVAL(Category__c, &apos;Leashing&apos;), 5,
											6)))))</formula>
